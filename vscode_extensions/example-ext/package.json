{
  "name": "example-ext",
  "publisher": "euewrqe",
  "displayName": "example_ext",
  "description": "sample code of vscode extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "example-ext.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "example-ext.showWarning",
        "title": "Show Warning"
      },
      {
        "command": "example-ext.showError",
        "title": "Show Error"
      },
      {
        "command": "example-ext.getUserInput",
        "title": "Get User Input"
      },
      {
        "command": "example-ext.showQuickPick",
        "title": "Show Quick Pick"
      },
      {
        "command": "example-ext.workWithEditor",
        "title": "Work with Editor"
      },
      {
        "command": "example-ext.insertText",
        "title": "Insert Text"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
